<Window x:Class="ListVIDPID.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Button Content="Get VID PID" Click="ButtonBase_OnClick"></Button>
            <TextBlock Text="{Binding VIDPIDList}"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
